                .note-inner
                  .note
              .note-wrap
                .note-inner
                  .note
              .note-wrap
                .note-inner
                  .note
          .title-head.grey
            h4 your best match
            .items-menu
              =image_tag "UD-icon-sub menu.svg"
          /.grid-block
