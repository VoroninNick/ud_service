      .full-width-panel.black
        =render partial: "partials/main_menu"
    .grid-block.large-9.medium-11.small-12.vertical.other-page.ease
      .grid-content.collapse.small-12.shrink
        .menu-bar.primary
          .primary-menu-button.close-primary-menu-button.hide-for-medium-only
            =image_tag "UD-icon-close.svg"
          .primary-menu-button.open-primary-menu-button.show-for-medium-only
            =image_tag "UD-icon-menu.svg"
      .grid-block
        .grid-content.large-9.medium-11.small-12.vertical.main-block
          .grid-block
            .notes
